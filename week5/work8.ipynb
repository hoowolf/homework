    "import re\n",
    "def validate_id_card(id):\n",
    "    pattern = r\"^[1-9]\\d{5}(19|20)(0[1-9]|1[0-2])(0[1-9]|[1-2]\\d|3[0-1])\\d{3}(\\d|X|x)$\"\n",
    "    match = re.match(pattern, id)\n",
    "    \n",
    "    if match:\n",
    "        print(\"Valid ID\")\n",
    "    else:\n",
    "        print(\"Invalid ID\")\n",
    "    \n",
    "validate_id_card('33010319900101000x')"
   ]
  }
