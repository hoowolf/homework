    "from datetime import datetime\n",
    "\n",
    "# 获取当前时间\n",
    "current_time = datetime.now()\n",
    "\n",
    "# 格式化时间为字符串\n",
    "formatted_time = current_time.strftime(\"%Y-%m-%d %H:%M:%S\")\n",
    "\n",
    "# 打印当前时间\n",
    "print(\"当前时间（精确到秒）:\", formatted_time)"
   ]
